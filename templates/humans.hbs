/* TEAM */
{{#each team}}
    {{{this.role}}}: {{{this.name}}}{{#if this.url}}
    Site: {{{this.url}}}{{/if}}{{#if this.twitter}}
    Twitter: {{{this.twitter}}}{{/if}}{{#if this.from}}
    From: {{{this.from}}}{{/if}}

{{/each}}
{{#if thanks}}

/* THANKS */
{{#each thanks}}
    {{{this.role}}}: {{{this.name}}}{{#if this.url}}
    Site: {{{this.url}}}{{/if}}{{#if this.twitter}}
    Twitter: {{{this.twitter}}}{{/if}}{{#if this.from}}
    From: {{{this.from}}}{{/if}}

{{/each}}
{{/if}}
{{#if contents}}

{{{contents}}}
{{/if}}

/* SITE */
    Last update: {{moment format="YYYY/MM/DD"}}{{#if standards}}
    Standards:{{#each standards}}
      - {{{this}}}{{/each}}{{/if}}{{#if components}}
    Components:{{#each components}}
      - {{{this}}}{{/each}}{{/if}}{{#if software}}
    Software:{{#each software}}
      - {{{this}}}{{/each}}{{/if}}
