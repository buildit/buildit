{
  "name": "buildit-website",
  "version": "4.2.0",
  "description": "buildit website",
  "main": "dist/index.html",
  "scripts": {
    "clean": "gulp clean",
    "commit": "git-cz",
    "build": "gulp build",
    "build-min": "gulp build --env local-dev-min",
    "build-staging": "gulp build --env staging",
    "build-prod": "gulp build --env production",
    "dev": "gulp",
    "start": "gulp --env local-dev-min",
    "test": "jest",
    "e2e": "cypress run",
    "lint": "eslint \"**/*.js\"",
    "spin-up-server": "http-server ./dist -c-1 -s",
    "test-watch": "jest --coverage --watch",
    "cypress": "cypress",
    "cypress:open": "cypress open",
    "cypress:cli": "cypress run",
    "cypress:travis": "npm run spin-up-server & cypress run",
    "pa11y": "./node_modules/.bin/pa11y-ci --sitemap http://localhost:8080/sitemap.xml  --sitemap-find '^https://.+?.buildit.digital' --sitemap-replace 'http://localhost:8080'",
    "pa11y:travis": "npm run spin-up-server & npm run pa11y",
    "ci": "npm run lint && npm run test && npm run cypress:travis && npm run pa11y"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint && npm test && npm run e2e",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/buildit/buildit.git"
  },
  "author": "buildit | wipro digital (https://buildit.wiprodigital.com)",
  "contributors": [
    "Matteo Pescarin",
    "James Nash",
    "Valerio Francescangeli",
    "Zane Cunningham",
    "Ana Escontrela",
    "Daniel Bull",
    "Daniel Wilkinson"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://digitalrig.atlassian.net/secure/RapidBoard.jspa?rapidView=229"
  },
  "homepage": "https://buildit.wiprodigital.com",
  "engines": {
    "node": ">= 8.10.0",
    "npm": ">= 5.8.0"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.26.0",
