
{
  "title": "buildit @ wipro digital",
  "description": "buildit@wiprodigital",
  "URL": "//buildit.wiprodigital.com",
  "paths": {
    "styles": {
      "src": [ "src/sass/**/*.scss" ],
      "dest": "dist/styles/"
    },
    "scripts": {
      "main": "src/scripts/main.js",
      "modules": "src/scripts/**/*.js",
      "src": "src/scripts/",
      "dest": "dist/scripts/"
    },
    "images": {
      "src": "static/images/",
      "dest": "dist/images/"
    },
    "pages": {
      "src": "pages/**/*.md",
      "dest": "dist/"
    },
    "templates": {
      "src": "templates/",
      "partials": "partials/"
    },
    "assets": {
      "src": [ "static/assets/*", "static/images/*.gif" ],
      "dest": "dist/"
    }
  }
}
