<div class="grav-c-page-header" resource="https://buildit.wiprodigital.com/thing/buildit-org" typeof="schema:Organization">
  <div class="grav-c-page-header__logo">
    {% if path %}
      <a href="/" rel="schema:url">
    {% else %}
      <h1>
    {% endif %}
        <svg role="img" class="grav-c-logotype" width="299" height="34">
          <title>buildit | wipro digital</title>
          <use xlink:href="#logo-buildit-logotype"></use>
        </svg>
    {% if path %}
      </a>
    {% else %}
        <link rel="schema:url" href="{{ env.url }}/">
      </h1>
    {% endif %}
  </div>
  <meta property="schema:name" content="buildit | wipro digital">
  <span hidden property="schema:logo" typeof="schema:ImageObject">
    <link rel="schema:url" href="/images/buildit-at-wipro-digital.png">
  </span>
  <button type="button" class="grav-c-toggle-menu" aria-pressed="false" aria-label="Do something">
    <span class="grav-c-toggle-menu__icon"></span>
  </button>
  <nav class="nav" typeof="schema:SiteNavigationElement">
    <ul>
      {% for navItem in colMainNav %}
      <li>
        <a class="grav-c-nav-link" rel="schema:url" href="/{{ navItem.path }}" property="schema:name">
          {{ navItem.title }}
        </a>
      </li>
      {% endfor %}
    </ul>
  </nav>
</div>
